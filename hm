                armDirrectionUp= inputScanner.nextInt();
                currentArmCoordinatesByX = currentArmCoordinatesByX - armDirrectionUp;
                missionOxygenTank = missionOxygenTank - armDirrectionUp++;

                }
                if (isArmGoingDown){

                }
                if (isArmGoingRight){

                }
                if (isArmGoingLeft){

                }

                }

                 while (isArmCoordinatesMovedByY){

                }























                while (isRoboticArmDeviceBValid) {
                    System.out.println("Ръка B е включена успешно ");
                    while (isRoboticArmDeviceBValid) {
                        isRoboticArmDeviceAValid = !isRoboticArmDeviceAValid;
                        isRoboticArmDeviceCValid = !isRoboticArmDeviceCValid;


                    }
                }

                while (isRoboticArmDeviceCValid) {
                    System.out.println("Ръка А е включена успешно ");
